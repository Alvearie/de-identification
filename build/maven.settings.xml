<?xml version="1.0" encoding="UTF-8"?>
<settings
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
	xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<servers>
		<server>
			<username>${env.ARTIFACTORY_USER}</username>
			<password>${env.ARTIFACTORY_KEY}</password>
			<id>central</id>
		</server>
		<server>
			<username>${env.ARTIFACTORY_USER}</username>
			<password>${env.ARTIFACTORY_KEY}</password>
			<id>snapshots</id>
		</server>
		<server>
			<username>${env.ARTIFACTORY_USER}</username>
			<password>${env.ARTIFACTORY_KEY}</password>
			<id>releases</id>
		</server>
    </servers>
	<profiles>
		<profile>
			<id>Merative_Artifactory</id>
			<repositories>
                <repository>
					<id>central</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-maven-virtual</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>snapshots</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-snapshot-maven-local</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<releases>
						<enabled>false</enabled>
					</releases>
				</repository>
				<repository>
					<id>releases</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-release-maven-local</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
					</releases>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-maven-virtual</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>snapshots</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-snapshot-maven-local</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<releases>
						<enabled>false</enabled>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>releases</id>
					<url>https://artifactory.commops.truvenhealth.com:443/artifactory/wh-de-id-release-maven-local</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
					</releases>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>
		<!--make the profile active all the time -->
		<activeProfile>Merative_Artifactory</activeProfile>
	</activeProfiles>
</settings>
